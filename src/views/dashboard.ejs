        </nav>
    </aside>
    <!-- Contenido principal -->
    <div class="content">
        <header>
            <h1>Dashboard - CamGreen</h1>
            <button id="logout">Cerrar Sesión</button>
        </header>
    </div>
    <script src="scriptdashboard.js"></script> <!-- Archivo de script del dashboard -->
